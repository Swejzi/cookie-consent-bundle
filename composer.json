{
    "name": "connectholland/cookie-consent-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for implementing Cookie Consent to comply to AVG/GDPR.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^1.10 || ^2.0",
        "doctrine/orm": "^2.5 || ^3.0",
        "symfony/asset": "^6.0 || ^7.0",
        "symfony/dependency-injection": "^6.0 || ^7.0",
        "symfony/doctrine-bridge": "^6.0 || ^7.0",
        "symfony/expression-language": "^6.0 || ^7.0",
        "symfony/form": "^6.0 || ^7.0",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "symfony/http-foundation": "^6.0 || ^7.0",
        "symfony/templating": "^6.0 || ^7.0",
        "symfony/translation": "^6.0 || ^7.0",
        "symfony/twig-bridge": "^6.0 || ^7.0",
        "symfony/twig-bundle": "^6.0 || ^7.0",
        "twig/twig": "^2.6 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5 || ^10.0 || ^11.0",
        "symfony/yaml": "^6.0 || ^7.0"
    },
    "prefer-stable": true,
    "autoload": {
        "exclude-from-classmap": ["/Tests/"],
        "psr-4": {
            "ConnectHolland\\CookieConsentBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConnectHolland\\CookieConsentBundle\\Tests\\": "/Tests"
        }
    },
    "config": {
         "sort-packages": true
    }
}
